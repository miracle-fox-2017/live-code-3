<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Movies</title>
  </head>
  <body>
    <h1>HALAMAN Movies</h1>

    <table>
  <tr>
    <th>ID</th>
    <th>name</th>
    <th>released year</th>
    <th>genre</th>
    <th>Production House</th>
    <th>edit</th>

  </tr>
      <%dataMovies.forEach(item =>{%>
  <tr>

    <td><%=item.id%></td>
    <td><%=item.name%></td>
    <td><%=item.released_year%></td>
    <td><%=item.genre%></td>
    <td><%=item.prodHouse%></td>
    <td>
      <a href="/movies/edit/<%=item.id%>">Edit</a>
    </td>
    <%})%>
  </tr>
  </body>
</html>
